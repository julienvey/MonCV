                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Maven, Ant, Hudson, Bamboo, Subversion (SVN), Git" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Modélisation :"
                android:textStyle="bold"
                android:paddingTop="5dip" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
