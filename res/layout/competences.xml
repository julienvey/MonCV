                android:layout_height="match_parent"
                android:text="Environnements de développement :"
                android:textStyle="bold"
                android:paddingTop="5dip" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Eclipse, Netbeans" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Architecture et frameworks :"
                android:textStyle="bold"
                android:paddingTop="5dip" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="OSGI/Distributed OSGI, Java EE (JSP, Servlets, EJB3), Spring, Hibernate, JPA, RMI, GraniteDS, Cairngorm" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="SOA :"
                android:textStyle="bold"
                android:paddingTop="5dip" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Clustering, WebServices (CXF), ESB, REST, Workflow/BPM (JBPM)" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Technologies Web :"
                android:textStyle="bold"
                android:paddingTop="5dip" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="JSF, JSP/Servlet, HTML/HTML5, CSS/CSS3, JavaScript, PHP" />

            <TextView
                android:layout_width="match_parent"
